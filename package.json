{
  "name": "filemanager-api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Angel Padrino <angel.padrino04@gmail.com",
  "license": "MIT",
  "scripts": {
    "start": "node dist",
    "dev": "npm-run-all build start",
    "dev:nodemon": "nodemon dev",
    "transpile": "swc ./src --out-dir dist --copy-files --sync",
    "build": "npm-run-all clean transpile",
    "test": "mocha -r @swc/register",
    "clean": "rm -rf dist",
    "watch:dev": "nodemon"
  },
  "nodemonConfig": {
    "exec": "yarn dev",
    "watch": [
      "src/*"
    ]
  },
  "dependencies": {
    "axios": "1.6.2",
    "express": "^4.18.2",
    "npm-run-all": "4.1.5"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.99",
    "@swc/register": "0.1.10",
    "chai": "4.3.10",
    "mocha": "10.2.0",
    "nodemon": "3.0.1"
  }
}